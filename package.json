{
  "name": "ciphering-cli-tool",
  "version": "1.0.0",
  "description": "CLI tool that encode and decode a text by 3 substitution ciphers: Caesar cipher, Atbash cipher and ROT-8 as variation of ROT-13 cipher",
  "main": "my_ciphering_cli",
  "type": "module",
  "scripts": {
    "start": "node my_ciphering_cli -c C1-C1-R0-A -i input.txt -o output.txt",
    "test": "jest ",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EkaterinaShep/ciphering-CLI-tool.git"
  },
  "author": "EkaterinaShep",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EkaterinaShep/ciphering-CLI-tool/issues"
  },
  "homepage": "https://github.com/EkaterinaShep/ciphering-CLI-tool#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.9"
  }
}
